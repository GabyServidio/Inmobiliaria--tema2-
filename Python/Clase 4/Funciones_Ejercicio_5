def es_divisible(num1, num2):
    if num1 % num2 == 0:
        print("el numero " + str(num1) + " es divisible por el numero " + str(num2))
    else:
        print("No son divisibles entre si")

x=int(input("Ingrese un numero: "))
y=int(input("Ingrese otro numero y vemos si son divisibles: "))

es_divisible(x, y)
